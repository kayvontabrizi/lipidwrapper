lipidwrapper --surface-equation "z = 250*numpy.sin(x*x/60000 +y*y/60000) * (-numpy.sqrt(x*x+y*y)/(560 * numpy.sqrt(2)) + 1)" --min-x -500 --max-x 500 --min-y -500 --max-y 500 --step-x 25 --step-y 25 --lipid-pdb-filename ./files/lipid_example.pdb --delete-clashing-lipids --fill-holes --clash-cutoff 1.0 --output-directory example2.from_equation.output_dir --number-of-processors 24 --use-disk-instead-of-memory --show-grid-points --create-triangle-tcl-file --clashing-potential-margin 15.0 --memory-optimization-factor 1
